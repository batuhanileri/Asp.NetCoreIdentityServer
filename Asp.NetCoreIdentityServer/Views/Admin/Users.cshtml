@model List<AppUser>

@{
    ViewData["Title"] = "Users";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Üyeler</h2>
<br />
<table class="table table-bordered table-striped">
    <tr>
        <td>User Id</td>
        <td>User Name</td>
        <td>User Email</td>
    </tr>
    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan="3">Üye Yoktur.</td>
        </tr>
    }
    else
    {
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }

    }
</table>
